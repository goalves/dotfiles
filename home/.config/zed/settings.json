{
  "agent": {
    "default_model": {
      "provider": "anthropic",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "Kanagawa",
  "ui_font_size": 16,
  "buffer_font_size": 13.0,
  "hour_format": "hour24",
  "terminal": {
    "shell": {
      "with_arguments": {
        "program": "/opt/homebrew/bin/fish",
        "args": ["--login"]
      }
    }
  },
  "load_direnv": "shell_hook",
  "lsp": {
    "protobuf-language-server": {
      "binary": {
        "path": "buf",
        "arguments": ["beta", "lsp"]
      }
    },
    "eslint": {
      "settings": {
        "workingDirectory": {
          "mode": "location"
        }
      }
    },
    "gopls": {
      "initialization_options": {
        "buildFlags": [
          "-tags=integration,integration_redis,postgres,ups_integration"
        ],
        "usePlaceholders": true
      }
    }
  }
}
