{
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "Ayu Dark",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "hour_format": "hour24",
  "terminal": {
    "shell": {
      "with_arguments": {
        "program": "/opt/homebrew/bin/fish",
        "args": ["--login"]
      }
    }
  },
  "lsp": {
    "gopls": {
      "initialization_options": {
        "buildFlags": [
          "-tags=integration,integration_redis,postgres,ups_integration"
        ],
        "usePlaceholders": true
      }
    }
  }
}
