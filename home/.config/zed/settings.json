{
  "context_servers": {
    "mcp-server-github": {
      "settings": {
        "github_personal_access_token": ""
      }
    }
  },
  "icon_theme": "Catppuccin Frapp√©",
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4-latest"
    },
    "version": "2"
  },
  "language_models": {
    "anthropic": {
      "version": "1",
      "api_url": "https://api.anthropic.com"
    },
    "ollama": {
      "api_url": "http://localhost:11434"
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "Kanagawa",
  "ui_font_size": 16,
  "buffer_font_size": 13.0,
  "hour_format": "hour24",
  "terminal": {
    "shell": {
      "with_arguments": {
        "program": "/opt/homebrew/bin/fish",
        "args": ["--login"]
      }
    }
  },
  "load_direnv": "shell_hook",
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "closureReturnTypeHints": {
            "enable": "always"
          }
        }
      }
    },
    "protobuf-language-server": {
      "binary": {
        "path": "buf",
        "arguments": ["beta", "lsp"]
      }
    },
    "eslint": {
      "settings": {
        "workingDirectory": {
          "mode": "location"
        }
      }
    },
    "gopls": {
      "initialization_options": {
        "buildFlags": [
          "-tags=integration,integration_redis,postgres,ups_integration"
        ],
        "usePlaceholders": true
      }
    }
  }
}
